<zk>
	<!-- Defines the application's name -->
	<preference>
		<name>org.zkoss.zk.ui.WebApp.name</name>
		<value>jmtest</value>
	</preference>

	<system-config>
		<id-generator-class>foo.jmtest.SequenceIdGenerator</id-generator-class>
	</system-config>

	<library-property>
		<name>org.zkoss.zul.Button.mold</name>
		<value>trendy</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.grid.rod</name>
		<value>true</value>
	</library-property>

	<library-property>
		<name>org.zkoss.zul.listbox.rod</name>
		<value>true</value>
	</library-property>
	<session-config>
		<timeout-uri>/timeout.zul</timeout-uri>
	</session-config>
</zk>
